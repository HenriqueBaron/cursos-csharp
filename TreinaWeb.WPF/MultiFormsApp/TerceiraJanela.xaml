<Window x:Class="MultiFormsApp.TerceiraJanela"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MultiFormsApp"
        mc:Ignorable="d"
        Title="TerceiraJanela" Height="300" Width="300">
    <Grid Margin="0,0,0,-1">
        <TextBlock x:Name="txtTexto"
                   HorizontalAlignment="Center" VerticalAlignment="Center"
                   FontSize="40" Text="DialogResult"/>
        <Button Content="Cancelar" Width="80" Height="20"
                Margin="20,230,190,20" Click="Cancelar_Click" />
        <Button Content="OK" Width="80" Height="20"
                Margin="190,230,20,20" Click="OK_Click"/>
    </Grid>
</Window>
