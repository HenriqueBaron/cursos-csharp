<Window x:Name="WndMain" x:Class="TreinaWeb.CSAvEx161.Apresentacao.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TreinaWeb.CSAvEx161.Apresentacao"
        mc:Ignorable="d"
        Title="Leitor de XML" Height="350" Width="524.188" Loaded="WndMain_Loaded">
    <Grid>
        <DataGrid x:Name="dgItens" Height="301" VerticalAlignment="Top" Margin="10,10,0,0" HorizontalAlignment="Left" Width="498">
            <DataGrid.Columns>
                <DataGridTextColumn Binding="{Binding Id}" ClipboardContentBinding="{x:Null}" Header="Id"/>
                <DataGridTextColumn Binding="{Binding Nome}" ClipboardContentBinding="{x:Null}" Header="Nome"/>
            </DataGrid.Columns>
        </DataGrid>

    </Grid>
</Window>
